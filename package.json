{
  "name": "ez-config",
  "version": "0.2.0",
  "description": "A simple environment-aware configuration utility exporting clean JS objects",
  "main": "index.js",
  "jsnext:main": "lib/index.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "build": "babel -d dst/ lib/",
    "prepublish": "npm run build",
    "test": "lab -a code --sourcemaps --transform test/transform.js",
    "coverage": "lab -a code --sourcemaps --transform test/transform.js -r html -o coverage.html"
  },
  "repository": {
    "type": "git",
    "url": "github.com/mshick/ez-config"
  },
  "keywords": [
    "es6",
    "hapijs",
    "config",
    "configuration",
    "pojo",
    "conf",
    "yaml",
    "json",
    "environment"
  ],
  "author": "Michael Shick",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mshick/ez-config/issues"
  },
  "homepage": "https://github.com/mshick/ez-config",
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-eslint": "^3.1.23",
    "code": "^1.4.1",
    "eslint": "^0.24.1",
    "eslint-config-defaults": "^3.1.0",
    "lab": "^5.13.0"
  },
  "dependencies": {
    "hoek": "^3.0.4",
    "js-yaml": "^3.3.1",
    "minimist": "^1.1.1"
  }
}
